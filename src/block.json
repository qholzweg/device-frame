{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/device-frame",
	"version": "0.1.0",
	"title": "Device frame",
	"category": "common",
	"icon": "smartphone",
	"description": "Frames your image with a mobile device screen",
	"attributes": {
		"image": {
			"type": "object"
		},
		"device": {
			"type":"string",
			"default": "iphone"
		}
	},
	"supports": {
		"html": false,
		"align": ["full"]
	},
	"textdomain": "device-frame",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
